{
  "name": "node-react",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"yarn run client\" \"node Server/servidor.js\"",
    "client": "cd client && yarn start",
    "server": "nodemon Server/servidor.js",
    "dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\""
  },
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
